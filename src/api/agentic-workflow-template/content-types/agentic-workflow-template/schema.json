{
  "kind": "collectionType",
  "collectionName": "agentic_workflow_templates",
  "info": {
    "singularName": "agentic-workflow-template",
    "pluralName": "agentic-workflow-templates",
    "displayName": "Agentic Workflow Template"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    },
    "webtools": {
      "enabled": true
    }
  },
  "attributes": {
    "url_alias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::webtools.url-alias",
      "configurable": false
    },
    "Hero": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.hero-section",
      "repeatable": false
    },
    "SEO": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false,
      "required": true
    },
    "Steps": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.how-it-works",
      "repeatable": false
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "detail_section": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.text-image-section",
      "repeatable": true
    },
    "Systemprompt": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.rich-text",
      "repeatable": true
    },
    "category": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "Consulting",
      "enum": [
        "PE",
        "Consulting",
        "Marketing",
        "Research",
        "MISC"
      ]
    },
    "icon": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "<svg width=\"48\" height=\"48\" viewBox=\"0 0 48 48\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">   <!-- Soft purple rounded square background [1] -->   <rect x=\"2\" y=\"2\" width=\"44\" height=\"44\" rx=\"8\" fill=\"#f6edff\"/>    <!-- Dividing lines for the 4 quadrants (Product, Price, Place, Promotion) -->   <path d=\"M24 12V36M12 24H36\" stroke=\"#a259f7\" stroke-width=\"2\" stroke-linecap=\"round\"/>",
      "required": true
    },
    "Title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    }
  }
}
