{
  "kind": "collectionType",
  "collectionName": "landing_pages",
  "info": {
    "singularName": "landing-page",
    "pluralName": "landing-pages",
    "displayName": "Landing Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "webtools": {
      "enabled": true
    }
  },
  "attributes": {
    "url_alias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::webtools.url-alias",
      "writable": true,
      "configurable": false,
      "editable": false,
      "visible": true,
      "unique": true
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "backgroundImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Hero": {
      "type": "component",
      "repeatable": false,
      "component": "shared.hero-section"
    },
    "FinalCTASection": {
      "type": "component",
      "repeatable": false,
      "component": "shared.cta"
    },
    "Features": {
      "type": "component",
      "repeatable": false,
      "component": "shared.features"
    },
    "Products": {
      "type": "component",
      "repeatable": true,
      "component": "shared.text-image-section"
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "Sections": {
      "type": "component",
      "repeatable": true,
      "component": "shared.section"
    }
  }
}
